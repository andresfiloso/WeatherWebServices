<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from PeliculaServiceImplService WSDL service -->

  <service name="PeliculaServiceImpl" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://impl.services.soap"/>
      <parameter name="wsdlServiceElement" value="PeliculaServiceImplService"/>
      <parameter name="schemaQualified" value="http://beans.soap,http://impl.services.soap"/>
      <parameter name="wsdlServicePort" value="PeliculaServiceImpl"/>
      <parameter name="className" value="soap.services.impl.PeliculaServiceImplSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="PeliculaServiceImpl"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="existePelicula" qname="operNS:existePelicula" xmlns:operNS="http://impl.services.soap" returnQName="retNS:existePeliculaReturn" xmlns:retNS="http://impl.services.soap" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:titulo" xmlns:pns="http://impl.services.soap" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="traerPeliculasPorActor" qname="operNS:traerPeliculasPorActor" xmlns:operNS="http://impl.services.soap" returnQName="retNS:traerPeliculasPorActorReturn" xmlns:retNS="http://impl.services.soap" returnType="rtns:Pelicula" xmlns:rtns="http://beans.soap" soapAction="" >
        <parameter qname="pns:actor" xmlns:pns="http://impl.services.soap" type="tns:Actor" xmlns:tns="http://beans.soap"/>
      </operation>
      <parameter name="allowedMethods" value="existePelicula traerPeliculasPorActor"/>

      <typeMapping
        xmlns:ns="http://beans.soap"
        qname="ns:Pelicula"
        type="java:soap.beans.Pelicula"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://beans.soap"
        qname="ns:Actor"
        type="java:soap.beans.Actor"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
